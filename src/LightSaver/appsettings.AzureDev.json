{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "System": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
      "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "OAuth": {
    "GoogleAuthServer": "https://accounts.google.com/o/oauth2/v2/auth",
    "GoogleTokenEndpoint": "https://oauth2.googleapis.com/token",
    "PickerScope": "https://www.googleapis.com/auth/photospicker.mediaitems.readonly",
    "ResponseType": "code",
    "RedirectUri": "https://localhost:8443/google/auth/google-callback"
  },
  "SessionExpiration": 10,
  "MaxImages": 30,
  "ConnectionStrings": {
    "Default": "Server=tcp:lightsaver-db-server.database.windows.net,1433;Initial Catalog=lightsaver-db;Authentication=Active Directory Managed Identity;"
  },
  "SaveMethod": "cloud",
  "AzureStorage": {
    "AccountName": "lightsaver",
    "ContainerName": "resources"
  }
}